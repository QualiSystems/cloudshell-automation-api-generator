<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="ApiResponse"
    targetNamespace="http://schemas.qualisystems.com/ResourceManagement/ApiCommandResult.xsd"
    elementFormDefault="qualified"
    xmlns="http://schemas.qualisystems.com/ResourceManagement/ApiCommandResult.xsd"
    xmlns:mstns="http://schemas.qualisystems.com/ResourceManagement/ApiCommandResult.xsd"
     xmlns:excns="http://schemas.qualisystems.com/ResourceManagement/ExportImportConfigurationSchema.xsd"
    xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <!-- run from 'Developer Command Prompt for VS2013' otherwise you will have so many changes so you may fall a victim of an 'IDIOT PART BELOW' -->

  <!-- xsd.exe ApiCommandResult.xsd /classes /namespace:QualiSystems.ResourceManagement.Service.APIService 
                                  DONT FORGET THE RESERVATION STATUS TAGS 
                                  and ReservationProvisioningStatus, IDIOT!                           -->
  
  <!-- 
  How to generate ApiCommandResult.cs file from ApiCommandResult.xsd
  ___________________________________________________________________
  1. Open console and navigate to the location where ApiCommandResult.xsd is located
  
  2. Run the following command:
     "xsd ApiCommandResult.xsd /c /namespace:QualiSystems.ResourceManagement.Service.APIService" and if everything is ok ApiCommandResult.cs will be generated
  * You can get error that  ApiCommandResult.cs is not available for writting even after closing it from VS.
  In that case one of the solutions is to delete ApiCommandResult.cs and re-created it again.
  
  3. Before you check in the changes (ApiCommandResult.cs) compare the generated version with the version on the server. There will be important attributes missing that need 
     to be added manually. 
     EXAMPLE:
     -> check enum ReservationStatus and ReservationProvisioningStatus
     Attributes: [XmlEnumAttribute("Pending")], [XmlEnumAttribute("Started")], [XmlEnumAttribute("In Use")] will be missing. Add them to the correct place just
     like it is on the server side.
     
  
  4. IMPORTANT NOTE:
  If you use "xsd ApiCommandResult.xsd /c" instead of "xsd ApiCommandResult.xsd /c /namespace:QualiSystems.ResourceManagement.Service.APIService" the .cs file
  will be created but it will not be wrapped in the correct namespace and that will cause compile problems.
  EXAMPLE COMPILE ERROR:
    QS.TestShell.Server.csproj:
    ResourceManagement\Plugins\Packaging\Importers\DataModelImporter.cs(368,24): err
    or CS0576: Namespace '<global namespace>' contains a definition conflicting with
    alias 'AttributeType'
  -->

  
  <xs:complexType name="ResponseInfo">
  </xs:complexType>

  <xs:complexType name="ApiCommandResult">
    <xs:sequence>
      <xs:element name="Error" minOccurs="1" maxOccurs="1" type="xs:string" />
      <xs:element name="ErrorCode" minOccurs="1" maxOccurs="1" type="xs:int" />
      <xs:element name="ResponseInfo" minOccurs="0" maxOccurs="1" type="mstns:ResponseInfo" />
    </xs:sequence>
    <xs:attribute name="CommandName" type="xs:string" use="required" />
    <xs:attribute name="Success" type="xs:boolean" use="required" />
  </xs:complexType>

  <xs:element name="Response" type="mstns:ApiCommandResult" />

  <!-- Get Resource Description -->
  <xs:simpleType name="AttributeType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="DateTime"/>
      <xs:enumeration value="Numeric"/>
      <xs:enumeration value="String"/>
      <xs:enumeration value="Boolean"/>
      <xs:enumeration value="Lookup"/>
      <xs:enumeration value="Password"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="AttributeUsage">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Set"/>
      <xs:enumeration value="Additional"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="ResourceLockInfo">
    <xs:attribute name="ReservationName" type="xs:string"/>
    <xs:attribute name="MachineName" type="xs:string"/>
    <xs:attribute name="Username" type="xs:string"/>
    <xs:attribute name="Created" type="xs:string"/>
  </xs:complexType>
  
  <xs:complexType name="TopologyShortInfo">
    <xs:sequence>
      <xs:element name="Type" type="TopologysType"/>
      <xs:element name="State" type="TopologysState"/>
    </xs:sequence>
    <xs:attribute name="Name" type="xs:string"/>
    <xs:attribute name="Alias" type="xs:string"/>
  </xs:complexType>
  
  <xs:complexType name="DomainInfo">
    <xs:complexContent>
      <xs:extension base="mstns:ResponseInfo">
        <xs:sequence>
          <xs:element name="Groups" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="Group" minOccurs="1" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:attribute name="Name" type="xs:string" use="required" />
                    <xs:attribute name="Description" type="xs:string" use="optional"/>
                    <xs:attribute name="Role" type="xs:string" use="required"/>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="Topologies" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="Topology" minOccurs="1" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:attribute name="Name" type="xs:string" use="required" />
                    <xs:attribute name="Description" type="xs:string" use="optional"/>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="Resources" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="Resource" minOccurs="1" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:attribute name="Name" type="xs:string" use="required"/>
                    <xs:attribute name="Path" type="xs:string" use="optional" />
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="Name" type="xs:string" use="required"/>
        <xs:attribute name="Description" type="xs:string" use="optional"/>
        <xs:attribute name="TopologiesFolder" type="xs:string" use="optional"/>
        <xs:attribute name="Archived " type="xs:string" use="optional"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:complexType name="ResourceInfo">
    <xs:complexContent>
      <xs:extension base="mstns:ResponseInfo">
        <xs:sequence>
          <xs:element name="VmDetails" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="VmCustomParams" minOccurs="0" maxOccurs="1">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element name="VmCustomParam" minOccurs="1" maxOccurs="unbounded">
                        <xs:complexType>
                          <xs:attribute name="Name" type="xs:string" use="required" />
                          <xs:attribute name="Value" type="xs:string" use="required"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
              <xs:attribute name="UID" type="xs:string" use="required" />
              <xs:attribute name="CloudProviderFullName" type="xs:string" use="required"/>
            </xs:complexType>
          </xs:element>
          <xs:element name="ResourceAttributes" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="ResourceAttribute" minOccurs="1" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:attribute name="Name" type="xs:string" use="required" />
                    <xs:attribute name="Value" type="xs:string" use="required"/>
                    <xs:attribute name="Type" type="AttributeType" use="required"/>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="Domains" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="Domain" minOccurs="1" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:attribute name="Name" type="xs:string" use="required" />
                    <xs:attribute name="Description" type="xs:string" use="optional"/>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="ChildResources" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="ResourceInfo" minOccurs="1" maxOccurs="unbounded" type="ResourceInfo" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="LockInfo" minOccurs="0" maxOccurs="1" type="ResourceLockInfo"/>
          <xs:element name="Connections" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
              <xs:attribute name="FullPath" type="xs:string" use="required"/>
              <xs:attribute name="Weight" type="xs:int" use="required"/>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="Name" type="xs:string" use="required"/>
        <xs:attribute name="DriverName" type="xs:string" use="optional"/>
        <xs:attribute name="FolderFullPath" type="xs:string" use="required"/>
        <xs:attribute name="Address" type="xs:string" use="required"/>
        <xs:attribute name="FullAddress" type="xs:string" use="required"/>
        <xs:attribute name="RootAddress" type="xs:string" use="required"/>
        <xs:attribute name="ResourceFamilyName" type="xs:string" use="required"/>
        <xs:attribute name="ResourceModelName" type="xs:string" use="required"/>
        <xs:attribute name="Locked" type="xs:boolean" use="required"/>
        <xs:attribute name="Excluded" type="xs:boolean" use="required"/>
        <xs:attribute name="Description" type="xs:string" use="required"/>
        <xs:attribute name="Permission" type="xs:string" use="required"/>
        <xs:attribute name="UniqeIdentifier" type="xs:string" use="required"/>
        <xs:attribute name="ResourceLiveStatusName" type="xs:string" use="required"/>
        <xs:attribute name="ResourceLiveStatusDescription" type="xs:string" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <!-- Get Resource Description -->


  <xs:complexType name="ResourceLiveStatusInfo">
    <xs:complexContent>
