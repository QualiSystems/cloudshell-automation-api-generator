              <xs:sequence>
                <xs:element name="ResourceAttribute" minOccurs="1" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:attribute name="Name" type="xs:string" use="required" />
                    <xs:attribute name="Value" type="xs:string" use="required"/>
                    <xs:attribute name="Type" type="AttributeType" use="required"/>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="LockInfo" minOccurs="0" maxOccurs="1" type="ResourceLockInfo"/>
          <xs:element name="Connections" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
              <xs:attribute name="FullPath" type="xs:string" use="required"/>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="Name" type="xs:string" use="required"/>
        <xs:attribute name="FolderFullPath" type="xs:string" use="required"/>
        <xs:attribute name="Address" type="xs:string" use="required"/>
        <xs:attribute name="FullAddress" type="xs:string" use="required"/>
        <xs:attribute name="RootAddress" type="xs:string" use="required"/>
        <xs:attribute name="ResourceFamilyName" type="xs:string" use="required"/>
        <xs:attribute name="ResourceModelName" type="xs:string" use="required"/>
        <xs:attribute name="Alias" type="xs:string" use="required"/>
        <xs:attribute name="Locked" type="xs:boolean" use="required"/>
        <xs:attribute name="Excluded" type="xs:boolean" use="required"/>
        <xs:attribute name="WillBeLocked" type="xs:boolean" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ActiveTopologyResourceInfo">
    <xs:complexContent>
      <xs:extension base="mstns:ResponseInfo">
        <xs:sequence>
          <xs:element name="ResourceAttributes" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="ResourceAttribute" minOccurs="1" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:attribute name="Name" type="xs:string" use="required" />
                    <xs:attribute name="Value" type="xs:string" use="required"/>
                    <xs:attribute name="Type" type="AttributeType" use="required"/>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="LockInfo" minOccurs="0" maxOccurs="1" type="ResourceLockInfo"/>
          <xs:element name="Connections" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
              <xs:attribute name="FullPath" type="xs:string" use="required"/>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="Name" type="xs:string" use="required"/>
        <xs:attribute name="FolderFullPath" type="xs:string" use="required"/>
        <xs:attribute name="Address" type="xs:string" use="required"/>
        <xs:attribute name="FullAddress" type="xs:string" use="required"/>
        <xs:attribute name="RootAddress" type="xs:string" use="required"/>
        <xs:attribute name="ResourceFamilyName" type="xs:string" use="required"/>
        <xs:attribute name="ResourceModelName" type="xs:string" use="required"/>
        <xs:attribute name="Alias" type="xs:string" use="required"/>
        <xs:attribute name="Locked" type="xs:boolean" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="TopologyAbstractResourceInfo">
    <xs:complexContent>
      <xs:extension base="mstns:ResponseInfo">
        <xs:sequence>
          <xs:element name="Attributes" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="AbstractResourceAttribute" minOccurs="1" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:attribute name="Name" type="xs:string" use="required" />
                    <xs:attribute name="Value" type="xs:string" use="required"/>
                    <xs:attribute name="Type" type="AttributeType" use="required"/>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="RequiredAttributes" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="AbstractResourceRequiredAttribute" minOccurs="1" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:attribute name="Name" type="xs:string" use="required" />
                    <xs:attribute name="Value" type="xs:string" use="required"/>
                    <xs:attribute name="Type" type="AttributeType" use="required"/>
                  </xs:complexType>
