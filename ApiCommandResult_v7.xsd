    <xs:attribute name="ReservationLiveStatusDescription" type="xs:string" use="required"/>
  </xs:complexType>
    
  <!-- Get Topology Description -->  
  <xs:complexType name="EndPointConnectionInfo">
    <xs:complexContent>
      <xs:extension base="mstns:ResponseInfo">
        <xs:sequence>
          <xs:element name="Routes" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="Route" minOccurs="0" maxOccurs="unbounded" type="RouteInfo"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>        
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="VisualConnectorsInfo">
    <xs:complexContent>
      <xs:extension base="mstns:ResponseInfo">
        <xs:sequence>
          <xs:element name="Connectors" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="Connector" minOccurs="0" maxOccurs="unbounded" type="Connector"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
